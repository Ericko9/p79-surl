{
  "name": "shorturl",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "start": "node ./src/app production",
    "dev": "node --watch ./src/app dev",
    "test": "jest --coverage --runInBand --watchAll=true -- __test__/router.test.js",
    "coverage": "jest --maxWorkers=4 --coverage"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "ejs": "^3.1.8",
    "express": "^4.22.1",
    "express-rate-limit": "^6.6.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/supertest": "^2.0.12",
    "drizzle-kit": "^0.31.8",
    "jest": "^29.2.0",
    "supertest": "^6.3.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "**/*.{js,ejs}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/coverage/"
    ]
  }
}
